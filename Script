DROP TABLE IF EXISTS `users`;

CREATE TABLE `users`
(
  `id` bigint
(20) unsigned NOT NULL AUTO_INCREMENT,
  `name` varchar
(255) COLLATE utf8mb4_unicode_ci NOT NULL,
  `email` varchar
(255) COLLATE utf8mb4_unicode_ci NOT NULL,
  `number` int
(11) DEFAULT NULL,
  `phoneNumber` int
(11) DEFAULT NULL,
  `residence` varchar
(255) COLLATE utf8mb4_unicode_ci DEFAULT NULL,
  `zipCode` varchar
(255) COLLATE utf8mb4_unicode_ci DEFAULT NULL,
  `area` varchar
(255) COLLATE utf8mb4_unicode_ci DEFAULT NULL,
  `type` varchar
(255) COLLATE utf8mb4_unicode_ci DEFAULT NULL,
  `birthDate` date DEFAULT NULL,
  `photo` varchar
(255) COLLATE utf8mb4_unicode_ci DEFAULT NULL,
  `course` varchar
(255) COLLATE utf8mb4_unicode_ci DEFAULT NULL,
  `curricularYear` int
(11) DEFAULT NULL,
  `enruledYear` int
(11) DEFAULT NULL,
  `school` varchar
(255) COLLATE utf8mb4_unicode_ci DEFAULT NULL,
  `identificationDocument` varchar
(255) COLLATE utf8mb4_unicode_ci DEFAULT NULL,
  `identificationNumber` int
(11) DEFAULT NULL,
  `nif` bigint
(20) DEFAULT NULL,
  `niss` bigint
(20) DEFAULT NULL,
  `sns` bigint
(20) DEFAULT NULL,
  `enee` varchar
(255) COLLATE utf8mb4_unicode_ci DEFAULT NULL,
  `educationalSupport` text COLLATE utf8mb4_unicode_ci,
  `eneeExpirationDate` date DEFAULT NULL,
  `firstLogin` tinyint
(1) DEFAULT NULL,
  `responsibleName` varchar
(255) COLLATE utf8mb4_unicode_ci DEFAULT NULL,
  `responsiblePhone` int
(11) DEFAULT NULL,
  `responsibleEmail` varchar
(255) COLLATE utf8mb4_unicode_ci DEFAULT NULL,
  `responsibleKin` varchar
(255) COLLATE utf8mb4_unicode_ci DEFAULT NULL,
  `emergencyName` varchar
(255) COLLATE utf8mb4_unicode_ci DEFAULT NULL,
  `functionalAnalysis` text COLLATE utf8mb4_unicode_ci,
  `emergencyPhone` int
(11) DEFAULT NULL,
  `emergencyEmail` varchar
(255) COLLATE utf8mb4_unicode_ci DEFAULT NULL,
  `emergencyKin` varchar
(255) COLLATE utf8mb4_unicode_ci DEFAULT NULL,
  `remember_token` varchar
(100) COLLATE utf8mb4_unicode_ci DEFAULT NULL,
  `coordinatorApproval` tinyint
(1) DEFAULT NULL,
  `servicesApproval` varchar
(15) COLLATE utf8mb4_unicode_ci DEFAULT NULL,
  `created_at` timestamp NULL DEFAULT NULL,
  `updated_at` timestamp NULL DEFAULT NULL,
  `secondEmail` varchar
(255) COLLATE utf8mb4_unicode_ci DEFAULT NULL,
  `departmentNumber` int
(10) DEFAULT NULL,
  `gender` varchar
(50) COLLATE utf8mb4_unicode_ci DEFAULT NULL,
  `dateEneeApproved` date DEFAULT NULL,
  `dateEneeRequested` date DEFAULT NULL,
  `password` varchar
(255) COLLATE utf8mb4_unicode_ci DEFAULT NULL,
  `typeApplication` varchar
(255) COLLATE utf8mb4_unicode_ci DEFAULT NULL,
  PRIMARY KEY
(`id`),
  UNIQUE KEY `users_email_unique`
(`email`),
  UNIQUE KEY `users_number_unique`
(`number`),
  UNIQUE KEY `users_phonenumber_unique`
(`phoneNumber`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_unicode_ci;

LOCK TABLES `users` WRITE;
/*!40000 ALTER TABLE `users` DISABLE KEYS */;

